#: kaboutkdedialog_p.cpp:46
#, kde-format
msgid ""
"<html><font size=\"5\">KDE - Be Free!</font><br /><b>Frameworks Version %1</"
"b></html>"
msgstr ""
"<html><font size=\"5\">KDE - Bebaslah!</font><br /><b>Versi Frameworks %1</"
